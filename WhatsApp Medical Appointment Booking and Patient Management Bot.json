    {
      "parameters": {
        "jsCode": "const now = new Date();\nconst appointments = $input.all();\nconst reminders = [];\n\nfor (const appointment of appointments) {\n  const appointmentDate = new Date(appointment.json.appointmentDateTime);\n  const timeDiff = appointmentDate - now;\n  const hoursDiff = timeDiff / (1000 * 60 * 60);\n  \n  // Check if appointment needs 24h reminder\n  if (hoursDiff <= 24 && hoursDiff > 23 && !appointment.json.reminder24hSent) {\n    reminders.push({\n      ...appointment.json,\n      reminderType: '24h',\n      message: `Reminder: You have an appointment tomorrow at ${appointmentDate.toLocaleString()}. ${appointment.json.instructions || ''}`\n    });\n  }\n  \n  // Check if appointment needs 2h reminder\n  if (hoursDiff <= 2 && hoursDiff > 1 && !appointment.json.reminder2hSent) {\n    reminders.push({\n      ...appointment.json,\n      reminderType: '2h',\n      message: `Reminder: Your appointment is in 2 hours at ${appointmentDate.toLocaleString()}. Please confirm or reschedule.`\n    });\n  }\n}\n\nreturn reminders.map(r => ({ json: r }));"
      },
